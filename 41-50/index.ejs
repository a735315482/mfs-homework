<body>
    原网页
    <script>
        function printout(text) {
            console.log(text);
        }

        function getItems(callback) {
            var xhr = new XMLHttpRequest();
            xhr.open("get", "/json", true);
            xhr.onreadystatechange = function() {
                if (xhr.readyState == 4 && xhr.status == 200) {
                    callback(xhr.responseText)
                }
            }
            xhr.send();
        }
        getItems(printout);
    </script>
</body>

</html>